{
  "name": "synthetix-deployments",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "synthetixio/synthetix-deployments",
  "author": "Synthetix",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:mainnet": "cannon run synthetix-omnibus:latest@main --chain-id 1 --provider-url https://mainnet.infura.io/v3/$INFURA_KEY",
    "start:goerli": "cannon run synthetix-omnibus:latest@main --chain-id 5 --provider-url https://goerli.infura.io/v3/$INFURA_KEY",
    "start:sepolia": "cannon run synthetix-omnibus:latest@main --chain-id 11155111 --provider-url https://sepolia.infura.io/v3/$INFURA_KEY",
    "start:optimism": "cannon run synthetix-omnibus:latest@main --chain-id 10 --provider-url https://optimism.infura.io/v3/$INFURA_KEY",
    "start:optimism-goerli": "cannon run synthetix-omnibus:latest@main --chain-id 420 --provider-url https://optimism-goerli.infura.io/v3/$INFURA_KEY",
    "start:base-goerli": "cannon run synthetix-omnibus:latest@main --chain-id 84531 --provider-url https://base-goerli.infura.io/v3/$INFURA_KEY",
    "build:andromeda": "cannon build omnibus-base-goerli-andromeda.toml --dry-run --upgrade-from synthetix-omnibus:latest@andromeda --chain-id 84531 --provider-url https://base-goerli.infura.io/v3/$INFURA_KEY | tee ./e2e/cannon-build.log",
    "start:andromeda": "cannon build omnibus-base-goerli-andromeda.toml --port 8545 --keep-alive --dry-run --upgrade-from synthetix-omnibus:latest@andromeda --chain-id 84531 --provider-url https://base-goerli.infura.io/v3/$INFURA_KEY | tee ./e2e/cannon-build.log",
    "fetch-deployments": "node ./e2e/fetch-deployments.js ./e2e/cannon-build.log",
    "deploy": "node ./e2e/deploy.js ./e2e/deployments/upgrade.ndjson",
    "test:andromeda": "DEBUG='e2e:*' mocha ./e2e/tests/omnibus-base-goerli-andromeda.toml/**/*.js",
    "pretty": "prettier . '**/*.toml' --write",
    "pretty:check": "prettier . '**/*.toml' --check",
    "cannon": "cannon"
  },
  "devDependencies": {
    "@iarna/toml": "^3.0.0",
    "@pythnetwork/pyth-evm-js": "^1.29.2",
    "@usecannon/builder": "2.10.6-alpha.4",
    "@usecannon/cli": "2.10.6-alpha.4",
    "debug": "^4.3.4",
    "ethers": "^5.7.2",
    "hardhat": "^2.19.0",
    "mocha": "^10.2.0",
    "prettier": "^3.1.0",
    "prettier-plugin-toml": "^1.0.0",
    "solc": "^0.8.22"
  },
  "mocha": {
    "timeout": "2m",
    "recursive": true,
    "bail": true,
    "async-only": true,
    "exit": true
  },
  "packageManager": "yarn@3.6.3"
}
